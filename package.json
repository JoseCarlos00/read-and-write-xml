{
	"name": "read-and-write-xml",
	"version": "2.2.1",
	"description": "App  xml",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"publish": "env-cmd electron-builder build --win -p always",
		"dist": "electron-builder build --win"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JoseCarlos00/read-and-write-xml"
	},
	"author": "Jose Carlos Aguirre Romero",
	"keywords": [
		"Electron",
		"electron-app",
		"XML",
		"nodejs",
		"javascript",
		"xml2js"
	],
	"publish": {
		"provider": "github",
		"owner": "JoseCarlos00",
		"repo": "read-and-write-xml"
	},
	"build": {
		"appId": "com.josecarlosaguirre.read-and-write-xml",
		"icon": "./icon.ico",
		"productName": "read-and-write-xml",
		"win": {
			"target": " nsis",
			"icon": "./icon.ico"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "JoseCarlos00",
				"repo": "read-and-write-xml"
			}
		],
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"directories": {
			"output": "dist"
		},
		"files": [
			"**/*",
			"!node_modules/*"
		],
		"extraResources": [
			{
				"from": "./icon.ico"
			}
		],
		"fileAssociations": [
			{
				"ext": "xml",
				"name": "read-and-write-xml",
				"role": "Editor"
			},
			{
				"ext": "shxmlP",
				"name": "read-and-write-xml",
				"role": "Editor"
			},
			{
				"ext": "shxml",
				"name": "read-and-write-xml",
				"role": "Editor"
			}
		]
	},
	"license": "ISC",
	"devDependencies": {
		"electron": "^32.2.0",
		"electron-builder": "^25.1.8",
		"standard": "^17.1.2"
	},
	"dependencies": {
		"electron-log": "^5.2.0",
		"electron-updater": "^6.3.9",
		"env-cmd": "^10.1.0",
		"xml2js": "^0.6.2"
	}
}
