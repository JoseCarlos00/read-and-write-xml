{
	"name": "read-and-write-xml",
	"version": "2.0.6",
	"description": "App  xml",
	"main": "main.js",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "env-cmd electron-forge publish"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JoseCarlos00/read-and-write-xml"
	},
	"author": "Jose Carlos Aguirre Romero",
	"keywords": [
		"Electron",
		"electron-app",
		"XML",
		"nodejs",
		"javascript",
		"xml2js"
	],
	"publish": {
		"provider": "github",
		"owner": "JoseCarlos00",
		"repo": "read-and-write-xml"
	},
	"build": {
		"appId": "com.josecarlosaguirre.read-and-write-xml",
		"icon": "./icon.ico",
		"productName": "read-and-write-xml",
		"win": {
			"target": "nsis"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "JoseCarlos00",
				"repo": "read-and-write-xml"
			}
		],
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"directories": {
			"output": "dist"
		},
		"files": [
			"**/*"
		],
		"fileAssociations": [
			{
				"ext": "xml",
				"name": "read-and-write-xml",
				"role": "Editor"
			},
			{
				"ext": "shxmlP",
				"name": "read-and-write-xml",
				"role": "Editor"
			},
			{
				"ext": "shxml",
				"name": "read-and-write-xml",
				"role": "Editor"
			}
		]
	},
	"license": "ISC",
	"devDependencies": {
		"@electron-forge/cli": "^7.5.0",
		"@electron-forge/maker-deb": "^7.5.0",
		"@electron-forge/maker-rpm": "^7.5.0",
		"@electron-forge/maker-squirrel": "^7.5.0",
		"@electron-forge/maker-zip": "^7.5.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
		"@electron-forge/plugin-fuses": "^7.5.0",
		"@electron-forge/publisher-github": "^7.5.0",
		"@electron/fuses": "^1.8.0",
		"electron": "^32.2.0",
		"standard": "^17.1.2"
	},
	"dependencies": {
		"electron-log": "^5.2.0",
		"electron-squirrel-startup": "^1.0.1",
		"electron-updater": "^6.3.9",
		"electron-updater-yaml": "^1.1.2",
		"env-cmd": "^10.1.0",
		"xml2js": "^0.6.2"
	}
}
